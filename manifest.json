{
  "manifest_version": 2,
  "name": "No Moments for Twitter",
  "version": "1.0.0",
  "description": "A chrome extension that hides the Moments button on the Twitter website.",
  "homepage_url": "http://github.com/ngoldman/no-moments",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*"
      ],
      "css": ["no-moments.css"]
    }
  ]
}
